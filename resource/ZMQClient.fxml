<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<AnchorPane fx:id="anchorPane" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.test.ZMQClientController">
    <children>
        <Button fx:id="button" layoutX="142.0" layoutY="27.0" mnemonicParsing="false" onAction="#portInit" prefHeight="25.0" prefWidth="139.0" text="Init" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="symbol" layoutX="14.0" layoutY="27.0" prefHeight="25.0" prefWidth="68.0" text="SYMBOL" />
      <TextField fx:id="period" layoutX="97.0" layoutY="27.0" prefHeight="25.0" prefWidth="33.0" text="1" />
      <Button layoutX="14.0" layoutY="68.0" mnemonicParsing="false" onAction="#send" prefHeight="25.0" prefWidth="270.0" text="Send Data" />
      <Label fx:id="info" layoutX="14.0" layoutY="269.0" prefHeight="17.0" prefWidth="576.0" text="..." />
      <RadioButton fx:id="minuteRadioButton" layoutX="300.0" layoutY="72.0" mnemonicParsing="false" selected="true" text="Minute data">
         <toggleGroup>
            <ToggleGroup fx:id="dataGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="tickRadioButton" layoutX="427.0" layoutY="72.0" mnemonicParsing="false" text="Tick" toggleGroup="$dataGroup" />
      <Button layoutX="375.0" layoutY="27.0" mnemonicParsing="false" text="Button" onAction="#test"/>
    </children>

</AnchorPane>
